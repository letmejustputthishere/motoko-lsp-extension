{
	"name": "motoko-lsp-client",
	"displayName": "motoko",
	"readme": "README.md",
	"description": "Reports Motoko compiler errors and provides code completion and refactoring tools.",
	"author": "Moritz Fuller",
	"publisher": "motoko-lsp-client",
	"license": "MIT",
	"version": "2.0.0",
	"icon": "serveimage.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/letmejustputthishere/motoko-lsp-extension"
	},
	"categories": [
		"Programming Languages",
		"Extension Packs",
		"Linters"
	],
	"keywords": [
		"dfx",
		"motoko",
		"dfinity"
	],
	"engines": {
		"vscode": "^1.33.0"
	},
	"activationEvents": [
		"onLanguage:motoko"
	],
	"main": "./client/out/extension",
	"contributes": {
		"languages": [
			{
				"id": "motoko",
				"aliases": [
					"motoko",
					"motoko"
				],
				"extensions": [
					"mo"
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "cd client && npm run update-vscode && cd .. && npm run compile",
		"compile": "tsc -b",
		"watch": "tsc -b -w",
		"postinstall": "cd client && npm install && cd .."
	},
	"devDependencies": {
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"tslint": "^5.16.0",
		"typescript": "^3.6.4"
	}
}
